	pages = {369},
	file = {Erdemir et al. - 2020 - Credible practice of modeling and simulation in he.pdf:/home/hellsbells/Zotero/storage/R7CA6MXL/Erdemir et al. - 2020 - Credible practice of modeling and simulation in he.pdf:application/pdf},
}

