@article{rateitschak_parameter_2012,
	title = {Parameter {Identifiability} and {Sensitivity} {Analysis} {Predict} {Targets} for {Enhancement} of {STAT1} {Activity} in {Pancreatic} {Cancer} and {Stellate} {Cells}},
	volume = {8},
