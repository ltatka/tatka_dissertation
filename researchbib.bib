@article{kobayashi_evolutionary_2010-2,
	title = {Evolutionary design of oscillatory genetic networks},
	volume = {76},
	copyright = {http://www.springer.com/tdm},
	issn = {1434-6028, 1434-6036},
	url = {http://link.springer.com/10.1140/epjb/e2010-00200-9},
	doi = {10.1140/epjb/e2010-00200-9},
	abstract = {The present study is devoted to the design and statistical investigations of dynamical gene expression networks. In our model problem, we aim to design genetic networks which would exhibit stable periodic oscillations with a prescribed temporal period. While no rational solution of this problem is available, we show that it can be eﬀectively solved by running a computer evolution of the network models. In this process, structural rewiring mutations are applied to the networks with inhibitory interactions between genes and the evolving networks are selected depending on whether, after a mutation, they closer approach the targeted dynamics. We show that, by using this method, networks with required oscillation periods, varying by up to three orders of magnitude, can be constructed by changing the architecture of regulatory connections between the genes. Statistical properties of designed networks, including motif distributions and Laplacian spectra, are considered.},
	language = {en},
	number = {1},
	urldate = {2024-04-05},
	journal = {Eur. Phys. J. B},
	author = {Kobayashi, Y. and Shibata, T. and Kuramoto, Y. and Mikhailov, A. S.},
	month = jul,
	year = {2010},
	pages = {167--178},
	file = {Kobayashi et al. - 2010 - Evolutionary design of oscillatory genetic network.pdf:/home/hellsbells/Zotero/storage/ZJ88G6QR/Kobayashi et al. - 2010 - Evolutionary design of oscillatory genetic network.pdf:application/pdf;s11084-015-9471-0.pdf:/home/hellsbells/Zotero/storage/TF6E74TL/s11084-015-9471-0.pdf:application/pdf},
}

