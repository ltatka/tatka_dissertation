@unpublished{shin_standards_nodate,
	title = {Standards, {Dissemination}, and {Best} {Practices} in {Systems} {Biology}},
	abstract = {The reproduciblity of scientiﬁc research is crucial to the success of the scientiﬁc method. Here we review the current best practices when publishing mechanistic models in systems biology. We recommend, where possible, to use software engineering strategies such as testing, veriﬁcation, validation, documentation, versioning, iterative development, and continuous integration. In addition, adhering to the FAIR modeling principles allows other scientists to collaborate and build oﬀ of each other’s work. Existing standards such as SBML, CellML, or SED-ML can greatly improve the likelihood that a published model is reproducible, especially since such models will be deposited in well-established model repositories. Where models are published in executable programming languages, the source code and their data should be published as open-source in public code repositories together with any documentation and testing code. For complex models we recommend container based solutions where any software dependencies and the run-time context can be easily replicated.},
	language = {en},
	author = {Shin, Janis and Porubsky, Veronica and Carothers, James and Sauro, Herbert M},
